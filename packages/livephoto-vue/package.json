{
  "name": "livephoto-vue",
  "version": "1.0.1",
  "description": "A Vue 3 Live Photo component with dual-mode support (Apple LivePhotosKit + Custom implementation)",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "vue",
    "vue3",
    "livephoto",
    "live-photo",
    "apple",
    "component",
    "typescript"
  ],
  "author": "ziwen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FoundDream/livephoto-vue.git"
  },
  "homepage": "https://livephoto-vue.vercel.app",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "livephotoskit": "^1.5.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vue-tsc": "^2.2.10"
  },
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  }
}
